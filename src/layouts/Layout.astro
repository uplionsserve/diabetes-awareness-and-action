---
import { Image } from "astro:assets"
import "../styles/styles.css"
import daaLogo from "../assets/branding/logo-daa-color.svg"
import daaLogoGlowing from "../assets/branding/logo-daa-color-glowing.svg"

interface Props {
	title?: string
	description?: string
	noSeo?: boolean
}

const { title, description, noSeo = false } = Astro.props
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title>
			{
				title
					? `${title} | Diabetes Awareness And Action`
					: "Diabetes Awareness And Action | U.P. Lions Serve"
			}
		</title>
		<meta name="description" content={description} />
		{
			noSeo && (
				<>
					<meta name="robots" content="noindex, nofollow" />
					<meta name="googlebot" content="noindex, nofollow" />
				</>
			)
		}
		<script
			type="text/javascript"
			src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"
		></script>

		<!-- Favicon -->
		<link
			rel="icon"
			type="image/png"
			sizes="32x32"
			href="/diabetes-awareness-and-action/media/favicon/logo32.png"
		/>
		<link
			rel="apple-touch-icon"
			sizes="180x180"
			href="/diabetes-awareness-and-action/media/favicon/logo180.png"
		/>

		<!-- Font initialization -->
		<link rel="preconnect" href="https://fonts.googleapis.com" />
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
		<link
			rel="stylesheet"
			href="https://fonts.googleapis.com/css2?family=Tajawal:wght@400;500;600;700;800;900"
		/>
		<link
			rel="stylesheet"
			href="https://fonts.googleapis.com/css2?family=Sanchez:wght@400"
		/>
		<script
			async
			src="https://www.googletagmanager.com/gtag/js?id=G-7PNP9VN2K9"
		></script>
		<script>
			window.dataLayer = window.dataLayer || []
			function gtag() {
				dataLayer.push(arguments)
			}
			gtag("js", new Date())

			gtag("config", "G-7PNP9VN2K9")
		</script></head
	>

	<!-- Google tag (gtag.js) -->

